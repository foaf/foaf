<?xml version="1.0" ?> 

<!--
 
very draft schema for foaf

-->


<rdf:RDF 
xmlns:dc="http://purl.org/dc/elements/1.1/"
xmlns:foaf="http://xmlns.com/foaf/0.1/"
xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
xmlns:wn="http://xmlns.com/wordnet/1.6/" 
xmlns:rdfs="http://www.w3.org/TR/WD-rdf-schema#">


<!-- 
classes:

wn:person
wn:document
wn:image?


--all defined in the wordnet ns - http://xmlns.com/wordnet/1.6/

now all subclassed into the foaf ns

-->

 <rdfs:Class
 rdf:about="http://xmlns.com/foaf/0.1/person"
 rdfs:label="person"
 rdfs:comment="a foaf subclass of wn person">
 
  <rdfs:subClassOf rdf:resource="http://xmlns.com/wordnet/1.6/person" />

 </rdfs:Class> 

 <rdfs:Class
 rdf:about="http://xmlns.com/foaf/0.1/document"
 rdfs:label="document"
 rdfs:comment="a foaf subclass of wn document">
 
  <rdfs:subClassOf rdf:resource="http://xmlns.com/wordnet/1.6/document" />

 </rdfs:Class> 



<!-- properties
  name
 homepage (dan C uses range= a description and gives it a dc title)
 mbox
 seeAlso
 assurance
 img
 workplaceHomepage
 livesWith
 knowsWell
 knows
 friend
 wife
 sister
 worksWith
 depiction
 firstName
 surname
 interest
 workOrgHomepage (Dan Connolly)
 publications

(depictions lead to image resources with title, description, format etc.)

dc properties are in a separate namespace

dc title
dc description
dc format
dc contributor (a person)
dc creator (a person)

wot stuff is in a separate namespace

-->



 <rdf:Property rdf:about="http://xmlns.com/foaf/0.1/name"
 rdfs:label="name"
 rdfs:comment="the full name of a person">

	<rdfs:domain rdf:resource="http://xmlns.com/wordnet/1.6/person" />
	<rdfs:range rdf:resource="http://www.w3.org/TR/WD-rdf-schema#Literal" />

 </rdf:Property>

 <rdf:Property rdf:about="http://xmlns.com/foaf/0.1/firstName"
 rdfs:label="firstName"
 rdfs:comment="the first name of a person">

	<rdfs:domain rdf:resource="http://xmlns.com/wordnet/1.6/person" />
	<rdfs:range rdf:resource="http://www.w3.org/TR/WD-rdf-schema#Literal" />

 </rdf:Property>

 <rdf:Property rdf:about="http://xmlns.com/foaf/0.1/surname"
 rdfs:label="surname"
 rdfs:comment="the surname of a person">

	<rdfs:domain rdf:resource="http://xmlns.com/wordnet/1.6/person" />
	<rdfs:range rdf:resource="http://www.w3.org/TR/WD-rdf-schema#Literal" />

 </rdf:Property>

 <rdf:Property rdf:about="http://xmlns.com/foaf/0.1/homepage"
 rdfs:label="homepage"
 rdfs:comment="the homepage of a person">

	<rdfs:domain rdf:resource="http://xmlns.com/wordnet/1.6/person" />
	<rdfs:range rdf:resource="http://www.w3.org/TR/WD-rdf-schema#Resource" />

 </rdf:Property>


 <rdf:Property rdf:about="http://xmlns.com/foaf/0.1/mbox"
